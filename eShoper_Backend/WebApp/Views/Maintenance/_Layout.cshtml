@{ 
    Layout = "/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.H2Title</h2>

<div>
    <h4>@ViewBag.H4Title</h4>
    <hr />
    <div class="row">
        <div class="col-md-1">
            @await Html.PartialAsync("_MaintenanceNav")
        </div>
        <div class="col-md-11">
            @RenderBody()
        </div>
    </div>
</div>

@section Scripts {
    @RenderSection("Scripts", required: false)
}
        
